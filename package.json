{
    "name": "bootstrap",
    "version": "1.0.0",
    "description": "Learning bootstrap",
    "main": "index.html",
    "scripts": {
        "start": "npm run watch:all",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lite": "lite-server",
        "scss": "node-sass -o css/ css/",
        "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
        "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ay7ot/bootstrap.git"
    },
    "author": "Ayomide Ibiteye",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Ay7ot/bootstrap/issues"
    },
    "homepage": "https://github.com/Ay7ot/bootstrap#readme",
    "dependencies": {
        "bootstrap": "^5.0.0",
        "bootstrap-social": "^5.1.1",
        "font-awesome": "^4.7.0"
    },
    "devDependencies": {
        "lite-server": "^2.6.1",
        "node-sass": "^7.0.1",
        "onchange": "^7.1.0",
        "parallelshell": "^3.0.1"
    }
}
